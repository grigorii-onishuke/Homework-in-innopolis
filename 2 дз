numbers = int(input())
last_numbers = numbers%10
if last_numbers == 1:
    print(numbers, end='')
    print(' рубль')
elif 1<last_numbers<5:
    print(numbers, end='')
    print(' рубля')
else:
    print(numbers, end='')
    print(' рублей')

